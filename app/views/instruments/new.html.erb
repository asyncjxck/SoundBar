<h2>New</h2>
<%= form_for @instrument do |f| %>
  <%= f.label :model %>
  <%= f.text_field :model %>
  <%= f.label :instrument_type %>
  <%= f.text_field :instrument_type %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  
  <%# need to figure out how to auto assign these %>
  <%= f.label :category %>
  <%= f.text_field :category_id %>
  <%= f.label :brand %>
  <%= f.text_field :brand_id %>
  <%= f.label :user %>
  <%= f.text_field :user_id %>
  <%= f.submit "Create Instrument for brand"%>
<% end %>
 